{
    "extends": [
        "../../.eslintrc.json"
    ],
    "parserOptions": {
        "project": "./tsconfig.json"
    },
    "rules": {
        "import-alias/import-alias": 0,
        "object-curly-newline": [
            "error",
            {
                "ObjectExpression": "always",
                "ObjectPattern": {
                    "multiline": true,
                    "minProperties": 2
                },
                "ImportDeclaration": "never",
                "ExportDeclaration": {
                    "multiline": true,
                    "minProperties": 2
                }
            }
        ],
        "object-property-newline": "error"
    }
}